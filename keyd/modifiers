f23+leftshift+leftmeta = overload(copilotheld, toggle(numpad))
leftalt = layer(command)
rightalt = layer(command)
leftmeta = layer(option)
leftcontrol = layer(meta)
rightcontrol = rightcontrol

[command:C]
left = A-left
right = A-right
up = A-up
down = A-down
backspace = A-backspace
delete = A-delete
tab = swapm(appswitch, A-tab)
h = M-h

[option:A]
left = C-left
right = C-right
up = C-up
down = C-down
backspace = C-backspace
delete = C-delete
tab = C-tab

[appswitch:A]
` = S-A-tab

[meta]
h = C-h

[command+option]
left = C-A-left
right = C-A-right
up = C-A-up
down = C-A-down
backspace = C-A-backspace
delete = C-A-delete
tab = C-A-tab
h = C-A-h

[numpad]
f23+leftshift+leftmeta = clear()
m = kp0
, = kpplus
. = kpdot
/ = kpslash
j = kp1
k = kp2
l = kp3
; = kpminus
u = kp4
i = kp5
o = kp6
p = kpasterisk
7 = kp7
8 = kp8
9 = kp9
0 = kp0
enter = kpenter
= = kpequal

[shift+numpad]
leftshift = clear()
rightshift = clear()

[copilotheld]
left = C-M-left
right = C-M-right
up = C-M-up
down = C-M-down
0 = numlock
